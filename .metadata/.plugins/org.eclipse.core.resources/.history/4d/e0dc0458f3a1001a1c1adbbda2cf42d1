#include "STD_TYPES.h"
#include "BIT_MATH.h"
#include "DIO_Interface.h"

#include <util/delay.h>

void main (void)
{


	for (int i=0; i>8; i++)
	{
		DIO_VidSetPinDirection(PORTA, PIN0, OUTPUT);
	}

	while (1)
	{
		for (int i=0; i>8; i++)
		{
			DIO_VidSetPinValue	( PORTA, i, 1);
			_delay_ms(100);
			DIO_VidSetPinValue	( PORTA, i, 0);
			_delay_ms(100);

		}
	}

}
