void LED_VidButterfly(u8 LOC_u8Cycles, u8 LOC_u8Speed);
{
	for (u8 i=0; i<LOC_u8Cycles; i++)

{
DIO_VidSetPortValue()(PORTA, 0b00011000 );
				_delay_ms(LOC_u8Speed);
				DIO_VidSetPortValue()(PORTA, 0b00100100 );
				_delay_ms(LOC_u8Speed);
				DIO_VidSetPortValue()(PORTA, 0b01000010 );
				_delay_ms(LOC_u8Speed);
				DIO_VidSetPortValue()(PORTA, 0b10000001 );
				_delay_ms(LOC_u8Speed);
				DIO_VidSetPortValue()(PORTA, 0b01000010 );
				_delay_ms(LOC_u8Speed);
				DIO_VidSetPortValue()(PORTA, 0b00100100 );
				_delay_ms(LOC_u8Speed);
				DIO_VidSetPortValue()(PORTA, 0b00011000 );
				_delay_ms(LOC_u8Speed);
}
}
